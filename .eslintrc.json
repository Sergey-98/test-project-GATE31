{
    "parser": "eslint/parser",
    "plugins": ["prettier", "import", "@eslint"],
    "extends": [
        "plugin:prettier/recommended",
        "prettier",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-debugger": 2,
        "no-console": 0
    }
}
